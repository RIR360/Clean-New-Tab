{
    "name": "Clean New Tab",
    "description": "Custom 'New Tab' extension designed for Chromium browser. Designed by RIR360",
    "version": "1.0",
    "manifest_version": 3,
    "chrome_url_overrides":{
        "newtab": "index.html"
    },
    "permissions": [
        "storage",
        "bookmarks"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "favicon-16.png",
        "128": "favicon-128.png"
    }
}